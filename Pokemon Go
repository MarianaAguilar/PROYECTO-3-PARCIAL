    text("Jugador de la izquierda presiona A para hacer daño", 450, 30);
   }
   if(key =='a' || key=='A'){
    image(teclaT,0,0);
    fill(255);
    textSize(30);
    text("HP "+" 50/100", 100, 100);
    textSize(30);
    fill(255);
    text("HP "+" 25/100",800,100);
    fill(#FF0303);
    textAlign(CENTER);
    textSize(30);
    text("Jugador de la derecha presiona T para hacer daño", 450, 30);
   }
   if(key =='t' || key=='T'){
    image(teclaT,0,0);
    fill(255);
    textSize(30);
    text("HP "+" 25/100", 100, 100);
    textSize(30);
    fill(255);
    text("HP "+" 25/100",800,100);
    fill(#FF0303);
    textAlign(CENTER);
    textSize(30);
    text("Jugador de la izquierda presiona I para hacer daño", 450, 30);
   }
   if(key =='i' || key=='I'){
    image(ganador6,-50,0);
    fill(255);
    textSize(30);
    text("HP "+" 25/100", 100, 100);
    textSize(30);
    fill(255);
    text("HP "+" 0/100",800,100);
    textSize(30);
    fill(#FF0303);
    text("PRESIONA R PARA REINICIAR",465,595);
   }
   if (keyPressed){
     if(key =='r' || key =='R'){
      pantalla = 0;
     }
   }
    break;
  }
 }  

class Pokemon {
  String nombre;
  int hp;
  int ataque;
  int defensa ;
  int elemento;
  int dibujo;
  
  Pokemon(String nombre_, int hp_, int ataque_, int defensa_, int elemento_ ,  int dibujo_){
  nombre = nombre_;
  hp = hp_;
  ataque = ataque_;
  defensa = defensa_;
  elemento = elemento_;
  dibujo = dibujo_;
}

