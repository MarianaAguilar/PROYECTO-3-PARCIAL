void setup(){
  background(0);
  size(900,600);
    inicio = loadImage("ljk_jhz7.jpg");   // Load the image into the program  
    mundo = loadImage("mundo.jpg");   // Load the image into the program  
    seleccion = loadImage("original.jpg");   // Load the image into the programloadImage("select.jpg");
    batalla = loadImage("Ioc2K.png");   // Load the image into the program
    teclaC=loadImage("1.jpg");
    teclaO=loadImage("2.jpg");
    teclaM=loadImage("3.jpg");
    teclaB=loadImage("4.jpg");
    teclaA=loadImage("5.jpg");
    teclaT=loadImage("6.jpg");
    ganador1=loadImage("ganador1.jpg");
    ganador2=loadImage("ganador2.jpg");
    ganador3=loadImage("ganador3.jpg");
    ganador4=loadImage("ganador4.jpg");
    ganador5=loadImage("ganador5.jpg");
    ganador6=loadImage("ganador6.jpg");
    
    MisteriosaAguilaNegra = new Pokemon("MISTERIOSA AGUILA NEGRA",10,6,12,4,0);
    SirenaMagica = new Pokemon("Sirena Magica",8,5,10,2,1);
    AlasDeAvion = new Pokemon("Alas De Avion",7,4,8,1,2);
    Veneno = new Pokemon("Veneno",5,3,6,3,3);
    LobilloSalvaje = new Pokemon("Lobillo Salvaje",3,2,4,5,4);
    PicoDeAcero = new Pokemon("Pico de Acero",1,1,2,6,5);
  }

void draw(){
  println(pantalla);
